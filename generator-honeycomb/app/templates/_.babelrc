{
  "presets": [
    <%_ if (includeReact) { _%>
    "react",
    <%_ } _%>
    "es2015"
  ],
  "plugins": [
    <%_ if (includeReact) { _%>
    "react-hot-loader/babel",
    <%_ } _%>
    "transform-runtime",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "sourceMaps": "inline"
    }
  }
}
