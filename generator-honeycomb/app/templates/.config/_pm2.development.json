{
  "apps": [{
    "name": "<%= name %>",
    "script": "src/server/server.js",
    "watch": ["src/server"],
    "env": {
      "NODE_ENV": "development"
    },
    "exec_interpreter": "babel-node",
    "combine_logs": true,
    "error_file": "logs/<%= name %>.error.log",
    "out_file": "logs/<%= name %>.out.log",
    "pid_file": "pids/<%= name %>.pid"
  }]
}
