{
  "apps": [{
    "name": "honeycomb-layout",
    "script": "src/index.js",
    "watch": true,
    "ignore_watch" : ["src/templates"],
    "exec_interpreter": "babel-node",
    "env": {
      "NODE_ENV": "development"
    },
    "merge_logs": true,
    "log_file": "logs/honeycomb-layout.combined.log",
    "error_file": "logs/honeycomb-layout.error.log",
    "out_file": "logs/honeycomb-layout.out.log",
    "pid_file": "pids/honeycomb-layout.pid"
  }]
}
