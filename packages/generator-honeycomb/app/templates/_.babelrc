{
  "presets": [
    "es2015"
  ],
  "plugins": [
    <%_ if (includePreact) { _%>
    [ "transform-react-jsx", { "pragma": "h" } ],
    <%_ } _%>
    "transform-runtime",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "sourceMaps": "inline"
    }
  }
}
